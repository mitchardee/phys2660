#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]){

  if(argc<2){
    printf("Enter an integer at the command line!\n");
    return -1;}

  int a = atoi(argv[1]);

  printf("dec = %i\n",a);

  //convert to hex

  printf("hex = %x\n",a);


  return 0;
}
